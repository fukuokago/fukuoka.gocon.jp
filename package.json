{
  "name": "fukuoka.gocon.jp",
  "version": "1.0.0",
  "description": "Go Conference in Fukuoka",
  "author": "linyows",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "npm run clean && nuxt-ts build",
    "clean": "rm -rf dist/*",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint": "tslint --project .",
    "precommit": "npm run lint",
    "test": "jest",
    "ci": "npm i && npm run lint && npm audit",
    "dist": "npm run build && gsutil cp -r dist/* gs://fukuoka.gocon.jp/",
    "install:gsutil": "curl https://sdk.cloud.google.com | bash && exec -l $SHELL && gcloud init"
  },
  "dependencies": {
    "@nuxt/vue-app": "^2.4.3",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/bulma": "^1.2.1",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/pwa": "^2.6.0",
    "@types/koa": "^2.0.48",
    "cross-env": "^5.2.0",
    "koa": "^2.6.2",
    "nuxt-i18n": "^5.8.3",
    "nuxt-ts-edge": "latest",
    "package": "^1.0.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.4",
    "vue-lazyload": "^1.2.6",
    "vue-property-decorator": "^7.3.0",
    "vuex-class": "^0.3.1",
    "yaml-loader": "^0.5.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "prettier": "1.14.3",
    "tslint-config-standard": "^8.0.1",
    "vue-jest": "^3.0.2"
  }
}
